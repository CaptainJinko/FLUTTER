//INHERITANCE
//Inheritance

void main(){
  Car myNormalCar = Car();
   
   print(myNormalCar.numberOfSeat); // printing numer of seat from the field or property
   myNormalCar.drive();   //calling method to print wheels turns on the console
 
 }
 
class Car{
  int numberOfSeat= 5;
  
  void drive(){
    print('wheels turn');
  }
}
-------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
 void main(){
  Car myNormalCar = Car();
   
   print(myNormalCar.numberOfSeat);     // printing numer of seat from the field or property
   myNormalCar.drive();                  //calling method to print wheels turns on the console   
     
 }
 
class Car{
  int numberOfSeat= 5;
  
  void drive(){
    print('wheels turn'); 
    
    
  }
}

