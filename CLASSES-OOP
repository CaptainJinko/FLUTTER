/* can can get rid off the syntax [this.height etc ] instead use the a short form inside the constructor like 
Human({this.height})*/


 void main(){
    Human Jenny = Human(height: 1.9, weight: 2.5);
    print(Jenny.height);
    print(Jenny.weight);
    
    print('   ');
    
    Human James = Human(height:15, weight: 1.7);
    print(James.height);
    print(James.weight);
  }

class Human{
  
  double height;
  double weight;
  
  Human({this.height, this.weight});
     
  
}

